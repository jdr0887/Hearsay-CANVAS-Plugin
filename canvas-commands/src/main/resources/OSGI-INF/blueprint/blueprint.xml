<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" default-activation="lazy">

  <command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
    <command>
      <action class="org.renci.hearsay.commands.canvas.PullFeaturesAction">
        <property name="canvasDAOBean" ref="canvasDAOBean" />
        <property name="hearsayDAOBean" ref="hearsayDAOBean" />
      </action>
    </command>
    <command>
      <action class="org.renci.hearsay.commands.canvas.PullVariantsAction">
        <property name="canvasDAOBean" ref="canvasDAOBean" />
        <property name="hearsayDAOBean" ref="hearsayDAOBean" />
      </action>
    </command>
    <command>
      <action class="org.renci.hearsay.commands.canvas.PullTranscriptsAction">
        <property name="canvasDAOBean" ref="canvasDAOBean" />
        <property name="hearsayDAOBean" ref="hearsayDAOBean" />
      </action>
    </command>
  </command-bundle>

  <reference id="hearsayCanonicalVariantDAO" interface="org.renci.hearsay.dao.CanonicalVariantDAO" />
  <reference id="hearsayFeatureDAO" interface="org.renci.hearsay.dao.FeatureDAO" />
  <reference id="hearsayGeneConditionAssertionDAO" interface="org.renci.hearsay.dao.GeneConditionAssertionDAO" />
  <reference id="hearsayGeneConditionDAO" interface="org.renci.hearsay.dao.GeneConditionDAO" />
  <reference id="hearsayGeneDAO" interface="org.renci.hearsay.dao.GeneDAO" />
  <reference id="hearsayGenomicRefSeqDAO" interface="org.renci.hearsay.dao.GenomicRefSeqDAO" />
  <reference id="hearsayGenomicVariantDAO" interface="org.renci.hearsay.dao.GenomicVariantDAO" />
  <reference id="hearsayIndividualDAO" interface="org.renci.hearsay.dao.IndividualDAO" />
  <reference id="hearsayIndividualVariantDAO" interface="org.renci.hearsay.dao.IndividualVariantDAO" />
  <reference id="hearsayMolecularConsequenceDAO" interface="org.renci.hearsay.dao.MolecularConsequenceDAO" />
  <reference id="hearsayRegionDAO" interface="org.renci.hearsay.dao.RegionDAO" />
  <reference id="hearsayResolvedConditionDAO" interface="org.renci.hearsay.dao.ResolvedConditionDAO" />
<!--   <reference id="hearsayPopulationFrequencyDAO" interface="org.renci.hearsay.dao.PopulationFrequencyDAO" /> -->
  <reference id="hearsayTranscriptAlignmentDAO" interface="org.renci.hearsay.dao.TranscriptAlignmentDAO" />
  <reference id="hearsayTranscriptRefSeqDAO" interface="org.renci.hearsay.dao.TranscriptRefSeqDAO" />
  <reference id="hearsayTranscriptVariantDAO" interface="org.renci.hearsay.dao.TranscriptVariantDAO" />
  <reference id="hearsayTranslationRefSeqDAO" interface="org.renci.hearsay.dao.TranslationRefSeqDAO" />
<!--   <reference id="hearsayVariantAssertionDAO" interface="org.renci.hearsay.dao.VariantAssertionDAO" /> -->

  <bean id="hearsayDAOBean" class="org.renci.hearsay.dao.HearsayDAOBean">
    <property name="canonicalVariantDAO" ref="hearsayCanonicalVariantDAO" />
    <property name="featureDAO" ref="hearsayFeatureDAO" />
    <property name="geneConditionAssertionDAO" ref="hearsayGeneConditionAssertionDAO" />
    <property name="geneConditionDAO" ref="hearsayGeneConditionDAO" />
    <property name="geneDAO" ref="hearsayGeneDAO" />
    <property name="genomicRefSeqDAO" ref="hearsayGenomicRefSeqDAO" />
    <property name="genomicVariantDAO" ref="hearsayGenomicVariantDAO" />
    <property name="individualDAO" ref="hearsayIndividualDAO" />
    <property name="individualVariantDAO" ref="hearsayIndividualVariantDAO" />
    <property name="molecularConsequenceDAO" ref="hearsayMolecularConsequenceDAO" />
<!--     <property name="populationFrequencyDAO" ref="hearsayPopulationFrequencyDAO" /> -->
    <property name="regionDAO" ref="hearsayRegionDAO" />
    <property name="resolvedConditionDAO" ref="hearsayResolvedConditionDAO" />
    <property name="transcriptAlignmentDAO" ref="hearsayTranscriptAlignmentDAO" />
    <property name="transcriptRefSeqDAO" ref="hearsayTranscriptRefSeqDAO" />
    <property name="transcriptVariantDAO" ref="hearsayTranscriptVariantDAO" />
    <property name="translationRefSeqDAO" ref="hearsayTranslationRefSeqDAO" />
<!--     <property name="variantAssertionDAO" ref="hearsayVariantAssertionDAO" /> -->
  </bean>

  <reference id="annotationGeneDAO" interface="org.renci.hearsay.canvas.annotation.dao.AnnotationGeneDAO" />
  <reference id="annotationGeneExternalIdsDAO" interface="org.renci.hearsay.canvas.annotation.dao.AnnotationGeneExternalIdsDAO" />
  <reference id="genomeRefDAO" interface="org.renci.hearsay.canvas.ref.dao.GenomeRefDAO" />
  <reference id="featureDAO" interface="org.renci.hearsay.canvas.refseq.dao.FeatureDAO" />
  <reference id="transcriptDAO" interface="org.renci.hearsay.canvas.refseq.dao.TranscriptDAO" />
  <reference id="transcriptMapsDAO" interface="org.renci.hearsay.canvas.refseq.dao.TranscriptMapsDAO" />
  <reference id="transcriptMapsExonsDAO" interface="org.renci.hearsay.canvas.refseq.dao.TranscriptMapsExonsDAO" />
  <reference id="refSeqGeneDAO" interface="org.renci.hearsay.canvas.refseq.dao.RefSeqGeneDAO" />
  <reference id="referenceClinicalAssertionsDAO" interface="org.renci.hearsay.canvas.clinvar.dao.ReferenceClinicalAssertionsDAO" />
  <reference id="refSeqCodingSequenceDAO" interface="org.renci.hearsay.canvas.refseq.dao.RefSeqCodingSequenceDAO" />
  <reference id="SNPFrequencyPopulationDAO" interface="org.renci.hearsay.canvas.genome1k.dao.SNPFrequencyPopulationDAO" />
  <reference id="HGNCGeneDAO" interface="org.renci.hearsay.canvas.hgnc.dao.HGNCGeneDAO" />
  <reference id="variants_61_2_DAO" interface="org.renci.hearsay.canvas.refseq.dao.Variants_61_2_DAO" />
  <reference id="maxVariantFrequencyDAO" interface="org.renci.hearsay.canvas.exac.dao.MaxVariantFrequencyDAO" />
<!--   <reference id="variantFrequencyDAO" interface="org.renci.hearsay.canvas.exac.dao.VariantFrequencyDAO" /> -->

  <bean id="canvasDAOBean" class="org.renci.hearsay.canvas.dao.CANVASDAOBean">
    <property name="annotationGeneDAO" ref="annotationGeneDAO" />
    <property name="annotationGeneExternalIdsDAO" ref="annotationGeneExternalIdsDAO" />
    <property name="genomeRefDAO" ref="genomeRefDAO" />
    <property name="featureDAO" ref="featureDAO" />
    <property name="transcriptDAO" ref="transcriptDAO" />
    <property name="transcriptMapsDAO" ref="transcriptMapsDAO" />
    <property name="transcriptMapsExonsDAO" ref="transcriptMapsExonsDAO" />
    <property name="refSeqGeneDAO" ref="refSeqGeneDAO" />
    <property name="refSeqCodingSequenceDAO" ref="refSeqCodingSequenceDAO" />
    <property name="referenceClinicalAssertionsDAO" ref="referenceClinicalAssertionsDAO" />
    <property name="SNPFrequencyPopulationDAO" ref="SNPFrequencyPopulationDAO" />
    <property name="HGNCGeneDAO" ref="HGNCGeneDAO" />
    <property name="variants_61_2_DAO" ref="variants_61_2_DAO" />
    <property name="maxVariantFrequencyDAO" ref="maxVariantFrequencyDAO" />
<!--     <property name="variantFrequencyDAO" ref="variantFrequencyDAO" /> -->
  </bean>

</blueprint>
